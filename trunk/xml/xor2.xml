<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!-- Xpresso definition for XOR combinational gate
	with two binary inputs -->
<gate type="xor2" width="100" height="50">
	<inputs>
		<port name="in1" x1="31" y1="15" x2="5" y2="15" />
		<port name="in2" x1="31" y1="35" x2="5" y2="35" />
	</inputs>
	
	<outputs>
		<port name="out" x1="79" y1="25" x2="95" y2="25" />
	</outputs>

	<svg href="svg/xor2.svg" />
</gate>
